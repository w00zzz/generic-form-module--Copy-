{
  "name": "react-pwa",
  "version": "2.3.0",
  "type": "module",
  "description": "Starter kit for modern web applications",
  "homepage": "https://react-pwa.surenatoyan.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/suren-atoyan/react-pwa.git"
  },
  "bugs": {
    "url": "https://github.com/suren-atoyan/react-pwa/issues",
    "email": "contact@surenatoyan.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host ",
    "https-preview": "serve dist",
    "prepare": "husky install && cp -n env/.shared .env &",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:check": "eslint --max-warnings=0 \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "5.0.0-alpha.156",
    "@mui/material": "^5.15.0",
    "@mui/system": "^5.15.0",
    "@mui/utils": "^5.15.0",
    "@mui/x-date-pickers": "^7.1.0",
    "@yudiel/react-qr-scanner": "^2.0.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "material-ui-confirm": "^3.0.16",
    "mui-file-input": "^4.0.5",
    "mui-nested-menu": "^3.3.0",
    "notistack": "^3.0.1",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-complex-tree": "^2.4.4",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-hotkeys-hook": "^4.4.1",
    "react-i18next": "^14.1.1",
    "react-imask": "^7.6.0",
    "react-number-format": "^5.3.4",
    "react-pwa": "file:",
    "react-router-dom": "^6.21.0",
    "recoil": "^0.7.7",
    "rsuite": "^5.64.0",
    "simplebar-react": "^3.2.4",
    "yup": "^1.4.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.12",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.9",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0"
    ]
